source [file dirname [info script]]/testing.tcl

needs cmd syslog

test syslog-1.1 {syslog no args} -body {
	syslog
} -returnCodes error -result {wrong # args: should be "syslog ?-facility cron|daemon|...? ?-ident string? ?-options int? ?debug|info|...? message"}

test syslog-1.2 {syslog normal} -body {
	syslog "test text"
} -result {}

test syslog-1.3 {syslog emerg priority} -body {
	syslog emerg "test text"
} -result {}

test syslog-1.4 {syslog alert priority} -body {
	syslog alert "test text"
} -result {}

test syslog-1.5 {syslog crit priority} -body {
	syslog crit "test text"
} -result {}

test syslog-1.6 {syslog error priority} -body {
	syslog error "test text"
} -result {}

test syslog-1.7 {syslog warning priority} -body {
	syslog warning "test text"
} -result {}

test syslog-1.8 {syslog notice priority} -body {
	syslog notice "test text"
} -result {}

test syslog-1.9 {syslog info priority} -body {
	syslog info "test text"
} -result {}

test syslog-1.10 {syslog debug priority} -body {
	syslog debug "test text"
} -result {}

test syslog-1.11 {syslog invalid priority} -body {
	syslog badpriority "test text"
} -returnCodes error -result {Unknown priority}

test syslog-1.12 {syslog too many non-option args} -body {
	syslog debug "test text" "test text2" "test text3"
} -returnCodes error -result {wrong # args: should be "syslog ?-facility cron|daemon|...? ?-ident string? ?-options int? ?debug|info|...? message"}



testreport
